@model AdminMensajeria.Models.OPE_SOLICITUD

@{
    ViewBag.Title = "Nuevo Envío";
}


<div class="form-horizontal">

    <div class="form-group">
        <div class="col-md-6">
            <div class="field">
                @Html.EditorFor(model => model.RequiereAcuse)
                <label for="RequiereAcuse">Solicitar Acuse Impreso</label>
            </div>
            <span class="AlertaDanger" id="RequiereAcuseVal"></span>
        </div>
        <div class="col-md-6">
        </div>
    </div>

    <!--Controles de producto de solicitud-->
    <div class="form-group">
        <div class="col-md-3">
            <div class="field">
                <input id="Descripcion" class="form-control input-sm" PlaceHolder="Sobre con membresía" maxlength="100" />
                <label for="Descripcion">Descripción de Producto (*)</label>
            </div>
            <span class="AlertaDanger" id="DescripcionVal"></span>
        </div>
        
        <div class="col-md-3">
            <div class="field">
                <input id="Folio" class="form-control input-sm" PlaceHolder="XXX001" maxlength="100" />
                <label for="Descripcion">Folio</label>
            </div>
            <span class="AlertaDanger" id="FolioVal"></span>
        </div>

        <div class="col-md-2">
            <div class="field">
                <input id="Cantidad" class="form-control input-sm" type="Number" PlaceHolder="1" maxlength="18" />
                <label for="Cantidad">Cantidad</label>
            </div>
            <span class="AlertaDanger" id="CantidadVal"></span>
        </div>

        <div class="col-md-2">
            <div class="field">
                @Html.DropDownList("IdUnidad", (IEnumerable<SelectListItem>)ViewBag.IdUnidad, "Elige una Unidad", htmlAttributes: new { @class = "form-control input-sm", maxlength = "18" })
                <label for="IdUnidad">Unidad de Medida</label>
            </div>
            <span class="AlertaDanger" id="TipoClientProveeVal"></span>
        </div>
        <div class="col-md-2">
            <div class="field">
                <input id="Peso" class="form-control input-sm" type="Number" PlaceHolder="1.50" maxlength="18" any="any" />
                <label for="Peso">Peso Kg.</label>
            </div>
            <span class="AlertaDanger" id="PesoVal"></span>
        </div>
    </div>

    <div class="form-group">
        <!--Controles de punto de recolección-->
        <div class="col-md-6">
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#Remitente">Remitente</a></li>
            </ul>
            <div class="tab-content" style="margin-top: 10px;">
                <div id="Remitente" class="tab-pane fade in active">

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                <input type="checkbox" id="OtraDireccionRec1" />
                                <label for="OtraDireccionRec1">Usar otra dirección</label>
                            </div>
                            <span class="AlertaDanger" id="OtraDireccionRec1Val"></span>
                        </div>
                        <div class="col-md-6 hidden">
                            <div class="field">
                                <input id="IdPuntoRec1" class="form-control input-sm" />
                                <label for="IdPuntoRec1">IdPuntoEnt</label>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                <input id="DescripcionPuntoRec1" class="form-control input-sm" placeholder="Sucursal Norte" maxlength="100" />
                                <label for="DescripcionPuntoRec1">Descripción (*)</label>
                            </div>
                            <span class="AlertaDanger" id="DescripcionPuntoRec1Val"></span>
                        </div>
                        <div class="col-md-6">
                            <div class="field">
                                <input id="ContactoPuntoRec1" class="form-control input-sm" placeholder="Juan Peréz" maxlength="100" />
                                <label for="ContactoPuntoRec1">Contacto</label>
                            </div>
                            <span class="AlertaDanger" id="ContactoPuntoRec1Val"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                <input id="CallePuntosRec1" class="form-control input-sm" placeholder="Lazaro Cardenas" maxlength="100" />
                                <label for="CallePuntosRec1">Calle (*)</label>
                            </div>
                            <span class="AlertaDanger" id="CallePuntosRec1Val"></span>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input id="NoExtPuntosRec1" class="form-control input-sm" placeholder="724" maxlength="20" />
                                <label for="NoExtPuntosRec1"># Exterior (*)</label>
                            </div>
                            <span class="AlertaDanger" id="NoExtPuntosRec1Val"></span>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input id="NoIntPuntosRec1" class="form-control input-sm" placeholder="14" maxlength="20" />
                                <label for="NoIntPuntosRec1"># Interior</label>
                            </div>
                            <span class="AlertaDanger" id="NoIntPuntosRec1Val"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-3">
                            <div class="field">
                                <input id="ReferenciaPuntosRec1" class="form-control input-sm" placeholder="Entre Calles A y B" maxlength="100" />
                                <label for="ReferenciaPuntosRec1">Referencia </label>
                            </div>
                            <span class="AlertaDanger" id="ReferenciaPuntosRec1Val"></span>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input id="c1Rec1" name="c1Rec1" autocomplete="nope" type="text" class="form-control input-sm" placeholder="09864" maxlength="20" />
                                <label for="c1Rec1">Código Postal (*)</label>
                            </div>
                            <span class="AlertaDanger" id="c1Rec1Val"></span>
                        </div>
                        <div class="col-md-6">
                            <div class="field">
                                <select id="IdColoniaRec1" class="form-control"></select>
                                <label for="IdColoniaRec1">Elige Una Colonia (*)</label>
                            </div>
                            <span class="AlertaDanger" id="IdColoniaRec1Val"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                <input type="text" disabled="disabled" id="MunicipioRec1" class="form-control input-sm" placeholder="Iztacalco" />
                                <label for="MunicipioRec1">Municipio</label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input type="text" disabled="disabled" id="EstadoRec1" class="form-control input-sm" placeholder="Ciudad de México" />
                                <label for="EstadoRec1">Estado</label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input type="text" disabled="disabled" id="PaisRec1" class="form-control input-sm" placeholder="México" />
                                <label for="PaisRec1">País</label>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-12">
                            <div class="field">
                                <input id="ObsPuntosRec1" class="form-control input-sm" placeholder="Observaciones" maxlength="500" />
                                <label for="ObsPuntosRec1">Observaciones</label>
                            </div>
                            <span class="AlertaDanger" id="ObsPuntosRec1Val"></span>
                        </div>
                    </div>


                </div>
            </div>
        </div>

        <!--Controles de punto de Entrega-->
        <div class="col-md-6">
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#Entrega">Entrega</a></li>
            </ul>
            <div class="tab-content" style="margin-top: 10px;">
                <div id="Entrega" class="tab-pane fade in active">

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                @Html.DropDownList("ListCategoria", (IEnumerable<SelectListItem>)ViewBag.ListCategoria, "Elige una Categoría", htmlAttributes: new { @class = "form-control" })
                                <label for="ListCategoria">Categoria (*)</label>
                            </div>
                            <span class="AlertaDanger" id="ListCategoriaVal"></span>
                        </div>
                        <div class="col-md-6">
                            <div class="field">
                                <select id="ListCliente" name="ListCliente" class="form-control input-sm">
                                    <option value="">Elige un Cliente</option>
                                </select>
                                <label for="ListCliente">Clientes (*)</label>
                            </div>
                            <span class="AlertaDanger" id="ListClienteVal"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                <input type="checkbox" id="OtraDireccionEnt1" />
                                <label for="OtraDireccionEnt1">Enviar a una dirección no registrada</label>
                            </div>
                            <span class="AlertaDanger" id="OtraDireccionEnt1Val"></span>
                        </div>
                        <div class="col-md-6">
                            <div class="field">
                                <input id="DestinatarioPuntosEnt1" class="form-control input-sm" placeholder="Juan Pérez" maxlength="100" />
                                <label for="DestinatarioPuntosEnt1">Destinarario</label>
                            </div>
                            <span class="AlertaDanger" id="DestinatarioPuntosEnt1Val"></span>
                        </div>
                        <div class="col-md-6 hidden">
                            <div class="field">
                                <input id="IdPuntoEnt1" class="form-control input-sm" />
                                <label for="IdPuntoEnt1">IdPuntoEnt</label>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                <input id="DescripcionPuntoEnt1" class="form-control input-sm" placeholder="Sucursal Norte" maxlength="100" />
                                <label for="DescripcionPuntoEnt1">Descripción (*)</label>
                            </div>
                            <span class="AlertaDanger" id="DescripcionPuntoEnt1Val"></span>
                        </div>
                        <div class="col-md-6">
                            <div class="field">
                                <input id="ContactoPuntoEnt1" class="form-control input-sm" placeholder="Juan Peréz" maxlength="100" />
                                <label for="ContactoPuntoEnt1">Contacto</label>
                            </div>
                            <span class="AlertaDanger" id="ContactoPuntoEnt1Val"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                <input id="CallePuntosEnt1" class="form-control input-sm" placeholder="Lazaro Cardenas" maxlength="100" />
                                <label for="CallePuntosEnt1">Calle (*)</label>
                            </div>
                            <span class="AlertaDanger" id="CallePuntosEnt1Val"></span>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input id="NoExtPuntosEnt1" class="form-control input-sm" placeholder="724" maxlength="20" />
                                <label for="NoExtPuntosEnt1"># Exterior (*)</label>
                            </div>
                            <span class="AlertaDanger" id="NoExtPuntosEnt1Val"></span>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input id="NoIntPuntosEnt1" class="form-control input-sm" placeholder="14" maxlength="20" />
                                <label for="NoIntPuntosEnt1"># Interior</label>
                            </div>
                            <span class="AlertaDanger" id="NoIntPuntosEnt1Val"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-3">
                            <div class="field">
                                <input id="ReferenciaPuntosEnt1" class="form-control input-sm" placeholder="Entre Calles A y B" maxlength="100" />
                                <label for="ReferenciaPuntosEnt1">Referencia </label>
                            </div>
                            <span class="AlertaDanger" id="ReferenciaPuntosEnt1Val"></span>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input id="c1Ent1" name="c1Ent1" autocomplete="nope" type="text" class="form-control input-sm" placeholder="09864" maxlength="20" />
                                <label for="c1Ent1">Código Postal (*)</label>
                            </div>
                            <span class="AlertaDanger" id="c1Ent1Val"></span>
                        </div>
                        <div class="col-md-6">
                            <div class="field">
                                <select id="IdColoniaEnt1" class="form-control"></select>
                                <label for="IdColoniaEnt1">Elige Una Colonia (*)</label>
                            </div>
                            <span class="AlertaDanger" id="IdColoniaEnt1Val"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6">
                            <div class="field">
                                <input type="text" disabled="disabled" id="MunicipioEnt1" class="form-control input-sm" placeholder="Iztacalco" />
                                <label for="MunicipioEnt1">Municipio</label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input type="text" disabled="disabled" id="EstadoEnt1" class="form-control input-sm" placeholder="Ciudad de México" />
                                <label for="EstadoEnt1">Estado</label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="field">
                                <input type="text" disabled="disabled" id="PaisEnt1" class="form-control input-sm" placeholder="México" />
                                <label for="PaisEnt1">País</label>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-12">
                            <div class="field">
                                <input id="ObsPuntosEnt1" class="form-control input-sm" placeholder="Observaciones" maxlength="500" />
                                <label for="ObsPuntosEnt1">Observaciones</label>
                            </div>
                            <span class="AlertaDanger" id="ObsPuntosEnt1Val"></span>
                        </div>
                    </div>

                </div>
            </div>
        </div>

    </div>


    <div class="form-group row">
        <div class="col-md-2">
            <button id="ValidaSolicitud" type="button" class="btn btn-primary btn-sm ">
                <span class="glyphicon glyphicon-floppy-disk"></span> Crear
            </button>
        </div>
        <div class="col-md-9">
            <span class="AlertaDanger" id="MensajeError"></span>
        </div>
        <div id="VolverCliente" class="col-md-1">
            <a href="@Url.Action("Index", "Solicitud")" class="btn btn-danger btn-sm">
                <span class="glyphicon glyphicon-floppy-remove"></span> Cancelar
            </a>
        </div>
    </div>




</div>

<!-- scripts generales-->
<script>
    $(document).ready(function () {
        //Funciones que se Cargan desde el principio
        ElegirPuntoRec1(@ViewBag.idpuntorec);
        ActivarInputsRec1(true);
        ActivarInputs(true);


        //Acciones para agregar selectpicker
        $('#IdUnidad, #IdColoniaRec1, #ListCategoria, #ListCliente, #IdColoniaEnt1').selectpicker({
            liveSearch: true
        });
        $("button").addClass("input-sm").css({ "font-size": "12px" });


        //Acciones para limpiar mensajes de Validación Producto

        $('#Descripcion').keyup(function () {
            $('#DescripcionVal').html('');
        });

        $('#Folio').keyup(function () {
            $('#FolioVal').html('');
        });

        $('#Cantidad').keyup(function () {
            $('#CantidadVal').html('');
        });

        $('#Peso').keyup(function () {
            $('#PesoVal').html('');
        });

        //Acciones para limpiar mensajes de Validación Punto de recolección

        $('#DescripcionPuntoRec1').keyup(function () {
            $('#DescripcionPuntoRec1Val').html('');
        });

        $('#ContactoPuntoRec1').keyup(function () {
            $('#ContactoPuntoRec1Val').html('');
        });

        $('#CallePuntosRec1').keyup(function () {
            $('#CallePuntosRec1Val').html('');
        });

        $('#NoExtPuntosRec1').keyup(function () {
            $('#NoExtPuntosRec1Val').html('');
        });

        $('#NoIntPuntosRec1').keyup(function () {
            $('#NoIntPuntosRec1Val').html('');
        });

        $('#ReferenciaPuntosRec1').keyup(function () {
            $('#ReferenciaPuntosRec1Val').html('');
        });

        $('#ObsPuntosRec1').keyup(function () {
            $('#ObsPuntosRec1Val').html('');
        });

        $('#c1Rec1').keyup(function () {
            $('#c1Rec1Val').html('');
            $("#MunicipioRec1").val('');
            $("#EstadoRec1").val('');
            $("#PaisRec1").val('');
        });

        //Acciones para limpiar mensajes de Validación Punto de entrega

        $('#DestinatarioPuntosEnt1').keyup(function () {
            $('#DestinatarioPuntosEnt1Val').html('');
        });

        $('#DescripcionPuntoEnt1').keyup(function () {
            $('#DescripcionPuntoEnt1Val').html('');
        });

        $('#ContactoPuntoEnt1').keyup(function () {
            $('#ContactoPuntoEnt1Val').html('');
        });

        $('#CallePuntosEnt1').keyup(function () {
            $('#CallePuntosEnt1Val').html('');
        });

        $('#NoExtPuntosEnt1').keyup(function () {
            $('#NoExtPuntosEnt1Val').html('');
        });

        $('#NoIntPuntosEnt1').keyup(function () {
            $('#NoIntPuntosEnt1Val').html('');
        });

        $('#ReferenciaPuntosEnt1').keyup(function () {
            $('#ReferenciaPuntosEnt1Val').html('');
        });

        $('#ObsPuntosEnt1').keyup(function () {
            $('#ObsPuntosEnt1Val').html('');
        });

        $('#c1Ent1').keyup(function () {
            $('#c1Ent1Val').html('');
            $("#MunicipioEnt1").val('');
            $("#EstadoEnt1").val('');
            $("#PaisEnt1").val('');
        });

        //Aciones que se disparan mediante eventos jquery Punto de Recolección
        $('#OtraDireccionRec1').change(function () {
            if ($("#OtraDireccionRec1").prop("checked") == true) {
                ActivarInputsRec1(false);
                LimpiarCamposRec1();
            }
            else {
                ElegirPuntoRec1(@ViewBag.idpuntorec);
                ActivarInputsRec1(true);
            }
        });

        $("#c1Rec1").blur(function () {
            if ($('#c1Rec1').val() != '') {
                var codigopostal = $('#c1Rec1').val();
                $('#c1Rec1Val').html('');
                ListarColoniasRec1(codigopostal);
            }
            else {
                $('#c1Rec1Val').html('');
                $('#c1Rec1Val').append('Este Campo es Obligatorio');
            }
        });

        $("#IdColoniaRec1").change(function () {
            var IdColonia = $("#IdColoniaRec1").val();
            if ($("#IdColoniaRec1").val == 0) {
                $("#MunicipioRec1").val('');
                $("#EstadoRec1").val('');
                $("#PaisRec1").val('');
                $("#IdColoniaRec1Val").html('Debe Elegir una Colonia');
            }
            else {
                ComplementoDomiclioRec1(IdColonia);
                $("#IdColoniaRec1Val").html('');
            }
        });

        //Aciones que se disparan mediante eventos jquery Punto de Entrega

        $('#ListCategoria').change(function () {
            if ($('#ListCategoria').val() != '') {
                var IdCategoria = $('#ListCategoria').val();
                $("#ListCategoriaVal").html('');
                ObtenerClientes(IdCategoria);
            }
            else {
                $("#ListCategoriaVal").html('');
                $("#ListCategoriaVal").append('Debe elegir una Categoría para ver los Clientes');
            }
        });

        $('#ListCliente').change(function () {
            if ($('#ListCliente').val() != '') {
                var IdCliente = $('#ListCliente').val();
                $("#ListClienteVal").html('');
                VerPuntos(IdCliente);
            }
            else {
                $("#ListClienteVal").html('');
                $("#ListClienteVal").append('Debe elegir un Cliente Para ver los Puntos de Entrega');
            }
        });

        $('#OtraDireccionEnt1').change(function () {
            if ($("#OtraDireccionEnt1").prop("checked") == true) {
                ActivarInputs(false);
                LimpiarCampos();
            }
            else {
                ActivarInputs(true);
                LimpiarCampos();
            }
        });


        $("#c1Ent1").blur(function () {
            if ($('#c1Ent1').val() != '') {
                var codigopostal = $('#c1Ent1').val();
                $('#c1Ent1Val').html('');
                ListarColonias(codigopostal);
            }
            else {
                $('#c1Ent1Val').html('');
                $('#c1Ent1Val').append('Este Campo es Obligatorio');
            }
        });


        $("#IdColoniaEnt1").change(function () {
            var IdColonia = $("#IdColoniaEnt1").val();
            if ($("#IdColoniaEnt1").val == 0) {
                $("#MunicipioEnt1").val('');
                $("#EstadoEnt1").val('');
                $("#PaisEnt1").val('');
                $("#IdColoniaEnt1Val").html('Debe Elegir una Colonia');
            }
            else {
                ComplementoDomiclio(IdColonia);
                $("#IdColoniaEnt1Val").html('');
            }
        });



        //Diaparador de boton crear
        $("#ValidaSolicitud").click(function () {

            //Comienza Validación de datos
            var ProductoValido = false;
            var PuntoEntregaValido = false;
            var PuntoRecoleccionValido = false;


            //validación de datos de producto
            if ($("#Descripcion").val().length == 0) {
                $("#DescripcionVal").html('');
                $("#DescripcionVal").append('Este Campo es Obligatorio');
            }

            if ($("#Descripcion").val().length > 100) {
                $("#DescripcionVal").html('');
                $("#DescripcionVal").append('Este Campo es puede contener maximo 100 caracteres');
            }

            if ($("#Folio").val().length > 100) {
                $("#FolioVal").html('');
                $("#FolioVal").append('Este Campo es puede contener maximo 100 caracteres');
            }

            if ($("#Cantidad").val().length > 18) {
                $("#CantidadVal").html('');
                $("#CantidadVal").append('Este Campo es puede contener maximo 18 caracteres');
            }

            if ($("#Peso").val().length > 18) {
                $("#PesoVal").html('');
                $("#PesoVal").append('Este Campo es puede contener maximo 18 caracteres');
            }

            if ($("#Descripcion").val().length != 0 && $("#Descripcion").val().length <= 100 &&
                $("#Descripcion").val().length <= 100 &&
                $("#Cantidad").val().length <= 18 && $("#Peso").val().length <= 18) {
                ProductoValido = true;
                console.log("Datos de Producto Validos");
            }
            else {
                console.log("Datos de Producto No Validos");
            }


            //validación de datos de Punto Recolección

            if ($('#IdPuntoRec1').val() != '') {
                //Agrega registro punto de Recoleccion de solicitud con un punto Existente

                if ($('#ObsPuntosRec1').val().length > 500) {
                    $("#ObsPuntosRec1Val").html('');
                    $("#ObsPuntosRec1Val").append('Este Campo es puede contener maximo 500 caracteres');
                }

                if ($('#ObsPuntosRec1').val().length <= 500) {
                    PuntoRecoleccionValido = true;
                    console.log("Datos de Punto de Recoleccion con punto Validos");
                }
                else {
                    console.log("Datos de Punto de Recoleccion con punto No Validos");
                }

            }
            else {
                //Agrega registro punto de Recoleccion de solicitud con domicilio particular
                if ($("#DescripcionPuntoRec1").val().length == 0) {
                    $("#DescripcionPuntoRec1Val").html('');
                    $("#DescripcionPuntoRec1Val").append('Este Campo es Obligatorio');
                }

                if ($("#DescripcionPuntoRec1").val().length > 100) {
