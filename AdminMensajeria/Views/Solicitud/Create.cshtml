                $('#ListCliente').selectpicker('refresh');
            },
            error: function () {
                console.log("No pudo acceder al metodo");
            }
        });
    }

    function VerPuntos(id) {
        $("#OperTitle").html('');
        $("#OperTitle").append('Lugares de Entrega')
        $("#OperBody").html('');
        $("#OperBody").load('@Url.Action("Puntos", "Solicitud")' + '/' + id);
        $('#OperModal').modal('show');
    }

    function ListarColonias(id) {
        $.ajax({
            url: '@Url.Action("ListaColonias", "Solicitud")' + '/' + id,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (Data) {
                $("#IdColoniaEnt1").html('');
                $("#IdColoniaEnt1").append('<option value ="0">Elige una Colonia</option>');
                $.each(Data,
                    function (index, propie) {
                        var content = '<option value="' + propie.idcolonia + '">' + propie.colonia + '</option>';
                        $("#IdColoniaEnt1").append(content);
                    });
                $('#IdColoniaEnt1').selectpicker('refresh');
            },
            error: function () {
                console.log("No pudo acceder al metodo");
            }
        });
    }

    function ComplementoDomiclio(id) {
        $.ajax({
            url: '@Url.Action("ComplementoDomicilio", "Solicitud")' + '/' + id,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (Data) {
                $("#MunicipioEnt1").val(Data.municipio);
                $("#EstadoEnt1").val(Data.estado);
                $("#PaisEnt1").val(Data.pais);
            },
            error: function () {
                console.log("No pudo acceder al metodo");
            }
        });
    }

    function ActivarInputs(bool) {
        $("#IdPuntoEnt1").prop('disabled', bool);
        $("#DescripcionPuntoEnt1").prop('disabled', bool);
        $("#ContactoPuntoEnt1").prop('disabled', bool);
        $("#CallePuntosEnt1").prop('disabled', bool);
        $("#NoExtPuntosEnt1").prop('disabled', bool);
        $("#NoIntPuntosEnt1").prop('disabled', bool);
        $("#ReferenciaPuntosEnt1").prop('disabled', bool);
        $("#c1Ent1").prop('disabled', bool);
    }

    function LimpiarCampos() {
        $("#IdPuntoEnt1").val('');
        $("#DescripcionPuntoEnt1").val('');
        $("#ContactoPuntoEnt1").val('');
        $("#CallePuntosEnt1").val('');
        $("#NoExtPuntosEnt1").val('');
        $("#NoIntPuntosEnt1").val('');
        $("#ReferenciaPuntosEnt1").val('');
        $("#c1Ent1").val('');
        $("#IdColoniaEnt1").html('');
        $("#MunicipioEnt1").val('');
        $("#EstadoEnt1").val('');
        $("#PaisEnt1").val('');
        $('#IdColoniaEnt1').selectpicker('refresh');
    }

</script>